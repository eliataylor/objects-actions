{
  "name": "object-actions-world-builder",
  "version": "0.1.0",
  "main": "src/WorldBuilder.ts",
  "scripts": {
    "start": "npx tsx src/main.ts --env-file=.env --action=objects-add --count=1",
    "import": "npx tsx tools/importer.ts",
    "objects-add": "npx tsx src/main.ts --env-file=.env --action=objects-add --count=1",
    "users-add": "npx tsx src/main.ts --env-file=.env --action=users-add --count=1",
    "object-add": "npx tsx src/main.ts --env-file=.env --action=object-add --type=resources --count=1",
    "delete-all": "npx tsx src/main.ts --env-file=.env --action=delete-all"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^22.10.3",
    "@types/tough-cookie": "^4.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@faker-js/faker": "9.1.0",
    "axios": "^1.7.2",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.1",
    "tough-cookie": "^4.1.4"
  }
}
